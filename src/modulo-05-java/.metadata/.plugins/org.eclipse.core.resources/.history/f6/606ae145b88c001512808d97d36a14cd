import br.com.cwi.crescer.LinkedList.Nodo;

public interface IList {

    void addFirst(String value);

    void addLast(String value);

    void removeFirst();

    void remove(int index);

    void adicionar(int index, String value);

    boolean isEmpty();

    List<String> list();

    String getFirst();

    String getLast();
    
}
